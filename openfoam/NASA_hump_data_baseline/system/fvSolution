FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      fvSolution;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

solvers
{
    p
    {
        solver          GAMG;
        tolerance       ABS_TOL;
        relTol          REL_TOL;
        smoother        GaussSeidel;
        nPreSweeps      0;
        nPostSweeps     2;
        cacheAgglomeration true;
        nCellsInCoarsestLevel 10;
        agglomerator    faceAreaPair;
        mergeLevels     1;
    }
    Phi
    {
        $p;
    }
    "(U|epsilon|k|omega)"
    {
        solver          smoothSolver;
        smoother        GaussSeidel;
        tolerance       ABS_TOL;
        relTol          REL_TOL
    }

    nuTilda
    {
        solver          smoothSolver;
        smoother        GaussSeidel;
        nSweeps         2;
        tolerance       ABS_TOL;
        relTol          REL_TOL;
    }
}

SIMPLE
{
    nNonOrthogonalCorrectors 0;
    pRefCell        0;
    pRefValue       0;

    residualControl
    {
        p               RES_TOL_P;
        U               RES_TOL_U;
        nuTilda         RES_TOL_NU;
    }
}

potentialFlow
{
    nNonOrthogonalCorrectors 0;
    PhiRefCell        0;
    PhiRefValue       0;
}

relaxationFactors
{
    fields
    {
        p               0.3;
    }
    equations
    {
        U               0.7;
        epsilon         0.7;
        k               0.7;
        nuTilda         0.7;
    }
}


